@model WashingCar.DAL.Entities.Vehicle

<dl class="row">
    <dt class = "col-sm-4">
        @Html.DisplayNameFor(model => model.Name)
    </dt>
    <dd class = "col-sm-10">
        @Html.DisplayFor(model => model.Name)
    </dd>
    <dt class = "col-sm-4">
        @Html.DisplayNameFor(model => model.Owner)
    </dt>
    <dd class = "col-sm-10">
        @Html.DisplayFor(model => model.Owner)
    </dd>
    <dt class = "col-sm-4">
        @Html.DisplayNameFor(model => model.NumberPlate)
    </dt>
    <dd class = "col-sm-10">
        @Html.DisplayFor(model => model.NumberPlate)
    </dd>
    <dt class = "col-sm-4">
        @Html.DisplayNameFor(model => model.CreationDate)
    </dt>
    <dd class = "col-sm-10">
        @Html.DisplayFor(model => model.CreationDate)
    </dd>
    @if (Model.DeliveryDate != null)
    {
        <dt class = "col-sm-4">
            @Html.DisplayNameFor(model => model.DeliveryDate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DeliveryDate)
        </dd>
    }
</dl>